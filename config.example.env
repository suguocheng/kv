# KV Store Configuration File - Example
# Copy this file to config.env and modify as needed

# Server Configuration
SERVER_CLIENT_PORT_BASE=9000
SERVER_PEER_PORT_BASE=8000
SERVER_NODE_COUNT=3
SERVER_HOST=localhost

# WAL Configuration
SERVER_MAX_WAL_ENTRIES=1000
SERVER_SNAPSHOT_INTERVAL=1000

# Data Paths
SERVER_DATA_BASE_PATH=data
SERVER_WAL_SUBDIR=wal
SERVER_RAFT_STATE_FILE=raft-state.pb
SERVER_SNAPSHOT_FILE=snapshot.pb

# Client Configuration
CLIENT_SERVERS=127.0.0.1:9000,127.0.0.1:9001,127.0.0.1:9002
CLIENT_HISTORY_DIR=history
CLIENT_HISTORY_FILE=kvcli_history

 